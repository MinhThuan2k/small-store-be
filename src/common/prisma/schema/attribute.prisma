model Attribute {
  attr_id     BigInt       @id @default(autoincrement())
  code        String
  name        String
  description String?
  created_at   DateTime      @default(now())
  updated_at   DateTime?     @updatedAt
  created_by   BigInt       
  updated_by   BigInt        
  deleted     Int           @default(0)
  deleted_at   DateTime?     

  products    ProductAttributeValue[]

  @@map("attributes")
}
