  "engines": {
    "node": ">=22.20.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate --schema=src/common/prisma/schema/",
    "db:push": "prisma db push --schema=src/common/prisma/schema/",
    "db:pull": "prisma db pull --schema=src/common/prisma/schema/",
    "db:studio": "prisma studio --schema=src/common/prisma/schema/",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "generate:env": "ts-node scripts/generate-env.ts"
  },
  "dependencies": {
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/static": "^8.1.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-fastify": "^11.0.12",
    "@nestjs/swagger": "^11.0.7",
    "@prisma/client": "^6.16.2",
    "@prisma/extension-accelerate": "^1.3.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "fs-extra": "^11.3.0",
    "googleapis": "^148.0.0",
    "ioredis": "^5.6.0",
    "lodash": "^4.17.21",
    "nestjs-cls": "^6.0.1",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.11",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.16.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "prisma": {
    "schema": "src/common/prisma/schema/"
  }
